{
  "schema": "schema.graphql",
  "documents": "GraphQL/**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "MyMarketManagerClient",
      "namespace": "MyMarketManager.GraphQL.Client",
      "records": {
        "inputs": true,
        "entities": true
      },
      "transportProfiles": [
        {
          "name": "Default",
          "default": "Http",
          "subscription": "WebSocket"
        },
        {
          "name": "InMemory",
          "default": "InMemory",
          "subscription": "InMemory"
        }
      ]
    }
  }
}